{
	"author": "Liferay Frontend Infrastructure Team <pt-frontend-infrastructure@liferay.com>",
	"dependencies": {
		"strip-ansi": "6.0.0",
		"xml": "^1.0.1"
	},
	"description": "A JUnit reporter for Jest and Liferay CI",
	"homepage": "https://github.com/liferay/liferay-frontend-projects/tree/master/projects/npm-tools/packages/jest-junit-reporter",
	"jest": {
		"testMatch": [
			"**/test/**/*.js"
		]
	},
	"license": "BSD-3-Clause",
	"main": "src/index.js",
	"name": "@liferay/jest-junit-reporter",
	"repository": {
		"directory": "projects/npm-tools/packages/jest-junit-reporter",
		"type": "git",
		"url": "https://github.com/liferay/liferay-frontend-projects"
	},
	"scripts": {
		"build": "true",
		"ci": "liferay-workspace-scripts ci",
